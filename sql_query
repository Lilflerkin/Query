SELECT 
    a.num,
    b.module_name,
    b.schedule_name,
    b.job_name,
    b.description,
    b.sla,
    b.frequency,
    DATE_ADD(LAST_DAY(ADD_MONTHS(from_unixtime(unix_timestamp()), -1)), a.num) AS executiondate
FROM (
    SELECT 1 AS num UNION ALL SELECT 2 UNION ALL SELECT 3 UNION ALL SELECT 4 UNION ALL 
    SELECT 5 UNION ALL SELECT 6 UNION ALL SELECT 7 UNION ALL SELECT 8 UNION ALL 
    SELECT 9 UNION ALL SELECT 10 UNION ALL SELECT 11 UNION ALL SELECT 12 UNION ALL 
    SELECT 13 UNION ALL SELECT 14 UNION ALL SELECT 15 UNION ALL SELECT 16 UNION ALL 
    SELECT 17 UNION ALL SELECT 18 UNION ALL SELECT 19 UNION ALL SELECT 20 UNION ALL 
    SELECT 21 UNION ALL SELECT 22 UNION ALL SELECT 23 UNION ALL SELECT 24 UNION ALL 
    SELECT 25 UNION ALL SELECT 26 UNION ALL SELECT 27 UNION ALL SELECT 28 UNION ALL 
    SELECT 29 UNION ALL SELECT 30 UNION ALL SELECT 31
) a
CROSS JOIN (
    SELECT 
        module_name,
        schedule_name,
        job_name,
        description,
        sla,
        frequency
    FROM bsl_cacl_risk_sox_db.cacl_jobs_details
    WHERE frequency = 'Daily'
) b
WHERE 
    DATE_ADD(LAST_DAY(ADD_MONTHS(from_unixtime(unix_timestamp()), -1)), a.num) >= '2024-02-01'
    AND DATE_ADD(LAST_DAY(ADD_MONTHS(from_unixtime(unix_timestamp()), -1)), a.num) <= LAST_DAY(from_unixtime(unix_timestamp()));
